   "source": [
    "# Ensuring all columns are treated as strings\n",
    "df['Customers Rated'] = df['Customers Rated'].astype(str)\n",
    "df['Price'] = df['Price'].astype(str)\n",
    "\n",
    "# Pre-processing the Rating, Customers Rated, and Price columns\n",
    "df['Rating'] = pd.to_numeric(df['Rating'], errors='coerce')\n",
    "df['Customers Rated'] = pd.to_numeric(df['Customers Rated'].str.replace(',', ''), errors='coerce')\n",
    "df['Price'] = pd.to_numeric(df['Price'].str.replace(',', ''), errors='coerce')\n",
    "\n",
    "# Checking NaNs and handle them by dropping\n",
    "df = df.dropna()"
   ]
