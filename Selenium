   "source": [
    "options = Options()\n",
    "options.add_argument('--headless')\n",
    "options.add_argument('--no-sandbox')\n",
    "options.add_argument('--disable-dev-shm-usage')\n",
    "driver = webdriver.Chrome(service=Service('D:\\chromedriver-win64\\chromedriver.exe'), options=options)\n",
    "\n",
    "start_time = time.time() #Setting the start time\n",
    "\n",
    "# Defining the URL of the Amazon Best Selling Books\n",
    "base_url = 'https://www.amazon.in/gp/bestsellers/books/ref=zg_bs_pg_'\n",
    "headers = {\n",
    "    'User-Agent': 'Mozilla/5.0 (Windows NT 10.0; Win64; x64) AppleWebKit/537.36 (KHTML, like Gecko) Chrome/107.0.0.0 Safari/537.36'\n",
    "}"
   ]
