   ],
   "source": [
    "# Set up Selenium WebDriver for Scrapy\n",
    "options_scrapy = Options()\n",
    "options_scrapy.add_argument('--headless')\n",
    "options_scrapy.add_argument('--no-sandbox')\n",
    "options_scrapy.add_argument('--disable-dev-shm-usage')\n",
    "driver_scrapy = webdriver.Chrome(service=Service('D:\\chromedriver-win64\\chromedriver.exe'), options=options_scrapy)\n",
    "\n",
    "books = []\n",
    "start_time_scrapy = time.time() #Setting the start time\n",
    "\n",
    "# Define the URL of the Amazon Best Selling Books\n",
    "base_url = 'https://www.amazon.in/gp/bestsellers/books/ref=zg_bs_pg_'\n",
    "\n",
    "# Loop through the first 5 pages\n",
    "for page_num in range(1, 6):\n",
    "    url = f\"{base_url}{page_num}/?ie=UTF8&pg={page_num}\"\n",
    "    driver_scrapy.get(url)\n",
    "    time.sleep(2)\n",
    "\n",
    "    scroll_count = 0\n",
    "    while scroll_count < 5:\n",
    "        driver_scrapy.execute_script(\"window.scrollTo(0, document.body.scrollHeight);\")\n",
    "        time.sleep(2)\n",
    "        scroll_count += 1\n",
    "\n",
    "    # Parsing the page source with BeautifulSoup\n",
    "    soup = BeautifulSoup(driver_scrapy.page_source, 'html.parser')\n",
    "    book_containers = soup.find_all('div', class_='zg-grid-general-faceout')\n",
    "    for book in book_containers:\n",
    "        # Finding the book name\n",
    "        name_container = book.find('div', class_=re.compile(r'_cDEzb_p13n-sc-css-line-clamp-1'))\n",
    "        name = name_container.text.strip() if name_container else 'Unknown'\n",
    "\n",
    "        # Finding the author\n",
    "        author_container = book.find('a', class_='a-size-small a-link-child')\n",
    "        if not author_container:\n",
    "            author_container = book.find('span', class_='a-size-small a-color-base')\n",
    "        author = author_container.text.strip() if author_container else 'Unknown'\n",
    "\n",
    "        # Finding the rating\n",
    "        rating_container = book.find('span', class_='a-icon-alt')\n",
    "        rating = rating_container.text.split()[0] if rating_container else '0'\n",
    "\n",
    "        # Finding the number of customers rated (more robust approach)\n",
    "        customers_rated_container = book.find_all('span', class_='a-size-small')\n",
    "        customers_rated = '0'\n",
    "        for container in customers_rated_container:\n",
    "            text = container.text.strip().replace(',', '')\n",
    "            if text.isdigit():\n",
    "                customers_rated = text\n",
    "                break\n",
    "\n",
    "        # Finding the price\n",
    "        price_container = book.find('span', class_=re.compile(r'_cDEzb_p13n-sc-price'))\n",
    "        price = price_container.text.replace('₹', '').replace(',', '').strip() if price_container else '0'\n",
    "\n",
    "        # Appending book details to the list if not already present\n",
    "        if [name, author, rating, customers_rated, price] not in books:\n",
    "            books.append([name, author, rating, customers_rated, price])\n",
    "\n",
    "# Close the Selenium WebDriver\n",
    "driver_scrapy.quit()\n",
    "\n",
    "scrapy_time = time.time() - start_time_scrapy #Processing time - Scrapy\n",
    "\n",
    "# Save results to CSV\n",
    "csv_file = \"amazon_books_scrapy.csv\"\n",
    "with open(csv_file, 'w', newline='', encoding='utf-8') as file:\n",
    "    writer = csv.writer(file)\n",
    "    writer.writerow([\"Name\", \"Author\", \"Rating\", \"Customers Rated\", \"Price\"])\n",
    "    writer.writerows(books)"
   ]
  },
